import java.util.Scanner;

    public class lacodois {
        public static void main (String[]args){
            int i, sum = 0;
            
            for(i=1; i<8; i++){
                if (i == 3 || i == 5){
                    continue;
                }
                sum += i;
                
            }
            System.out.println("A soma de 1, 2, 4, 6 e 7 Ã© " + sum + ".");
        }
    }
    
